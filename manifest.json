{
    "name": "ui工具",
    "version": "0.0.1",
    "description": "ui helper",
    "permissions": [
        "storage",
        "activeTab",
        "contextMenus",
        "clipboardRead"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_title": "ui工具",
        "default_icon": {
            "16": "images/get_started16.png",
            "32": "images/get_started32.png",
            "48": "images/get_started48.png",
            "128": "images/get_started128.png"
        }
    },
    "chrome_url_overrides": {},
    "icons": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
    },
    "commands": {
        "active": {
            "suggested_key": {
                "default": "Ctrl+I",
                "mac": "Ctrl+I"
            },
            "description": "active"
        }
    },
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "css": [
                "insert.css"
            ]
        }
    ],
    "manifest_version": 2
}